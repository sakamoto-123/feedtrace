<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
	</array>
	<!-- iCloud配置 -->
	<key>CKContainerIdentifiers</key>
	<array>
		<string>iCloud.cn.iizhi.BabyDaily</string>
	</array>
	<key>NSUbiquitousContainers</key>
	<dict>
		<key>iCloud.cn.iizhi.BabyDaily</key>
		<dict>
			<key>NSUbiquitousContainerIsDocumentScopePublic</key>
			<false/>
			<key>NSUbiquitousContainerName</key>
			<string>BabyDaily</string>
			<key>NSUbiquitousContainerSupportedFolderLevels</key>
			<string>Any</string>
		</dict>
	</dict>
	<!-- Photo Library Permission: For selecting photos to record baby's growth -->
	<key>NSPhotoLibraryUsageDescription</key>
	<string>We need access to your photo library to select photos for recording your baby's growth moments</string>
</dict>
</plist>
